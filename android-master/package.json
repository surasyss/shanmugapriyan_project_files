{
  "name": "MobileApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "android": "react-native run-android",
    "android:release": "cp .env.prod .env.production && react-native run-android --variant=Release",
    "ios": "react-native run-ios",
    "ios:release": "cp .env.prod .env.production && react-native run-ios --configuration Release",
    "ios:device": "react-native run-ios --device",
    "build:ios": "cp .env.prod .env.production && react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:android": "cp .env.prod .env.production && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --sourcemap-output android/app/src/main/assets/release.bundle.map",
    "build-sandbox:ios": "cp .env.sandbox .env.production && react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-sandbox:android": "cp .env.sandbox .env.production && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "deploy-sandbox:android": "./deploy/android/sandbox/deploy.sh"
  },
  "dependencies": {
    "@gurukumparan/react-native-android-inapp-updates": "^1.0.25",
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/datetimepicker": "^2.3.1",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-community/progress-view": "^1.3.1",
    "@react-native-community/viewpager": "^3.3.0",
    "@sentry/react-native": "^1.2.0",
    "accordion-collapse-react-native": "^0.2.7",
    "axios": "^0.19.0",
    "buffer": "^5.4.3",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "pushy-react-native": "^1.0.14",
    "react": "16.9.0",
    "react-native": "0.61.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-branch": "^5.6.2",
    "react-native-calendars": "^1.214.0",
    "react-native-camera": "^3.11.1",
    "react-native-device-info": "^5.5.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-fs": "^2.16.2",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-image-crop-picker": "^0.36.4",
    "react-native-image-picker": "^4.1.1",
    "react-native-image-progress": "^1.1.1",
    "react-native-image-to-pdf": "^1.2.0",
    "react-native-image-view": "^2.1.8",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-mixpanel": "^1.1.10",
    "react-native-modal": "^11.5.3",
    "react-native-modal-datetime-picker": "^8.5.3",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-paper": "^3.2.1",
    "react-native-pdf": "^6.3.0",
    "react-native-progress": "^4.0.2",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "^1.13.3",
    "react-native-screens": "^2.0.0-alpha.34",
    "react-native-scrollable-tab-view": "^1.0.0",
    "react-native-share": "^3.0.0",
    "react-native-super-grid": "^4.1.3",
    "react-native-svg": "^12.1.0",
    "react-native-user-agent": "^2.1.0",
    "react-native-vector-icons": "6.6.0",
    "react-native-version-check": "^3.4.3",
    "react-native-webview": "^11.23.1",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.9.0",
    "react-navigation-transitions": "^1.0.12",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/runtime": "7.7.2",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "24.9.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "jest": "24.9.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "0.56.3",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
